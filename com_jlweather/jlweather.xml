<?xml version="1.0" encoding="utf-8"?>
<extension type="component" version="1.6.0" method="upgrade">
	<name>JLWeather</name>
	<creationDate>March, 2011</creationDate>
	<author>JoomLine</author>
	<authorEmail>sale@joomline.ru</authorEmail>
	<authorUrl>http://www.joomline.ru</authorUrl>
	<copyright>Copyright 2013 JoomLine.ru All rights reserved</copyright>
	<license>GNU/GPL v.3 or later.</license>
	<version>2.5.1</version>
	<description>COM_JLWEATHER_XML_DESCRIPTION</description>
	
	<files folder="site">
		<filename>controller.php</filename>
		<filename>router.php</filename>
		<filename>index.html</filename>
		<filename>jlweather.php</filename>
		<folder>img</folder>
		<folder>models</folder>
		<folder>views</folder>
	</files>
		<languages folder="languages/site">
			<language tag="en-GB">en-GB/en-GB.com_jlweather.ini</language>
			<language tag="ru-RU">ru-RU/ru-RU.com_jlweather.ini</language>
			<language tag="uk-UA">uk-UA/uk-UA.com_jlweather.ini</language>
		</languages>
	<administration>
		<menu>COM_JLWEATHER_MAIN_MENU</menu>
		<files folder="admin">
			<filename>config.xml</filename>
			<filename>controller.php</filename>
			<filename>index.html</filename>
			<filename>jlweather.php</filename>
			<folder>views</folder>
		</files>
		<languages folder="languages/admin">
			<language tag="ru-RU">ru-RU/ru-RU.com_jlweather.sys.ini</language>
            <language tag="ru-RU">ru-RU/ru-RU.com_jlweather.ini</language>
            <language tag="en-GB">en-GB/en-GB.com_jlweather.sys.ini</language>
            <language tag="en-GB">en-GB/en-GB.com_jlweather.ini</language>
			<language tag="uk-UA">uk-UA/uk-UA.com_jlweather.sys.ini</language>
            <language tag="uk-UA">uk-UA/uk-UA.com_jlweather.ini</language>
		</languages>		
	</administration>
</extension>